const handleMoveUp = () => {
    const newTargetKeys = [...targetKeys];
    const index = targetKeys.findIndex(key => key === newTargetKeys[0]);
    if (index > 0) {
      [newTargetKeys[index - 1], newTargetKeys[index]] = [newTargetKeys[index], newTargetKeys[index - 1]];
      setTargetKeys(newTargetKeys);
    }
  };

  const handleMoveDown = () => {
    const newTargetKeys = [...targetKeys];
    const index = targetKeys.findIndex(key => key === newTargetKeys[newTargetKeys.length - 1]);
    if (index < newTargetKeys.length - 1) {
      [newTargetKeys[index + 1], newTargetKeys[index]] = [newTargetKeys[index], newTargetKeys[index + 1]];
      setTargetKeys(newTargetKeys);
    }
  };


 <div style={{ marginTop: 20, textAlign: 'center' }}>
        <Button
          type="primary"
          onClick={handleMoveUp}
          disabled={targetKeys.length === 0 || targetKeys.indexOf(targetKeys[0]) === 0}
        >
          Move Up
        </Button>
        <Button
          type="primary"
          onClick={handleMoveDown}
          style={{ marginLeft: 8 }}
          disabled={targetKeys.length === 0 || targetKeys.indexOf(targetKeys[targetKeys.length - 1]) === targetKeys.length - 1}
        >
          Move Down
        </Button>
</div>
